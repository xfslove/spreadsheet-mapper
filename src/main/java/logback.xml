<?xml version="1.0" encoding="UTF-8"?>
<configuration>

  <appender value="consoleAppender" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d [%t] %-5p %m%n</pattern>
    </encoder>
  </appender>

  <logger value="spreadsheet.mapper"/>

  <root level="info">
    <appender-ref ref="consoleAppender" />
  </root>

</configuration>
